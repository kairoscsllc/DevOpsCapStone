
from selenium import webdriver
from selenium.webdriver.chrome.options import Options
from selenium.webdriver.common.by import By
import time


class WriteURL:
    def testcase1():
        options = Options()
        options.headless = True 
        driver = webdriver.Chrome(options=options)
        driver.get("https://www.kairoscs.com/contact/")
        print ("If you see this message there is at least 1 COVID19 case in one county on every continent.  Using Selenium and Python on AWS to hit the website", " " , driver.current_url)
        print (driver.title)

    def testcase2():
        options = Options()
        options.headless  = True 
        driver = webdriver.Chrome(options= options)
        driver.get("https://www.mlb.com")
        print (driver.current_url)
        print (driver.title)        

WriteURL.testcase1()
WriteURL.testcase2()

